    "jest-axe": "^6.0.0",
    "lerna": "^8.0.2",
    "lerna-audit": "^1.3.3",
    "lint-staged": "^12.3.1",
    "lodash": "^4.17.21",
    "next": "14.2.25",
    "next-router-mock": "0.9.12",
    "nock": "^13.5.0",
    "picocolors": "1.0.0",
    "prettier": "^2.5.1",
    "pretty": "^2.0.0",
    "react-i18next": "^12.2.0",
    "testcafe": "^3.6.2",
    "testcafe-reporter-html": "^1.4.6",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.3"
  }
}
