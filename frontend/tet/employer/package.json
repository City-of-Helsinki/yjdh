{
  "name": "@frontend/te-empl",
  "version": "1.0.0",
  "scripts": {
    "dev": "PORT=3000 node ../../start.js",
    "build": "next build",
    "start": "NODE_ENV=production PORT=3000 node ../../start.js ",
    "pre-commit": "lint-staged -c ../../.lintstagedrc.js",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "npx jest --runInBand --no-cache --passWithNoTests",
    "test:debug-dom": "npx cross-env DEBUG_PRINT_LIMIT=1000000 yarn test",
    "test:staged": "yarn test --watchAll=false --findRelatedTests",
    "test:coverage": "yarn test:debug-dom --verbose --coverage",
    "lint": "next lint"
  },
  "dependencies": {
    "@frontend/shared": "*",
    "babel-plugin-import": "^1.13.3",
    "next": "^11.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.3.0",
    "next-plugin-custom-babel-config": "^1.0.5",
    "next-transpile-modules": "^9.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.34.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "eslint-config-adjunct": "^4.11.1",
    "typescript": "^4.5.5"
  }
}
